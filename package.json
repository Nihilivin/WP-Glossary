  "main": "index.js",
  "scripts": {
    "build:assets": "grunt rebuildAssets",
    "build:assets:watch": "grunt watch:assets",
    "build:rollup": "npm run build:rollup:clean && rollup -c rollup.config.js",
    "build:rollup:clean": "rm -rf ./assets/dist/* && rm -rf ./assets/tmp/*",
    "build:rollup:watch": "npm run build:rollup:clean && rollup -c rollup.config.js -w",
    "lint": "tslint --project ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "grunt test",
    "test:front": "npm run test:jest:unit && test:jest:integration",
    "test:jest:unit": "jest -c jest.unit.js",
    "test:jest:unit:watch": "npm run test:jest:unit -- --watch",
    "test:jest:integration": "jest -c jest.integration.js",
    "test:jest:integration:watch": "npm run test:jest:integration -- --watch",
    "test:prepare": "npm run test:prepare:drop-db; npm run test:prepare:init-db",
    "test:prepare:drop-db": "mysql -u root -proot -e 'DROP DATABASE `wp-test`;'",
    "test:prepare:init-db": "./bin/install-wp-tests.sh wp-test root root"
  },
